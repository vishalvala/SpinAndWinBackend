    $('#addReferAmount').submit(function(e){
    console.log("e ::::: ",e);
    e.preventDefault();
    var amount=$('#amount').val();
    // console.log(amou)
    // alert(amou);
    // alert($('form').serialize())
    var formData = new FormData(this);
    // alert(amount);
    console.log("formData  22 :::: ", formData);
    $.ajax({
      url: '<%=BaseUrl%>/addReferAmount',
      type: 'post',
      data: {amount : amount },
      // enctype: 'multipart/form-data',
      // cache:false,
      // contentType: false,
      // processData: false,
      success: (data)=>{
        if(data.error == 'Y'){
          console.log("error :::::::: ", data.msg);
          $('#messageerror').html(data.msg);
        }else{
          location.reload();
        }
      },
      error: (error)=>{
        console.log("error :::::::: ", error);
      }
    })
  })


    $("#checkboxVerification").change(function() {
      var url = '<%=BaseUrl%>/verficationOnOFF/';
      if(this.checked) {
        url += '1';
      }else{
        url += '0';
      }
      $.ajax({
            url: url,
            type: 'GET',
            success: (response)=>{
              console.log(response);
              var response = JSON.parse(response);
              var msg = '';
              if(response.status == 'active'){
                msg = "Enabled! All COD Status Activate!";
              }else{
                msg = "Disabled! All COD Status InActivate!"
              }
              swal(msg, {
                icon: "success",
              });
            },
            error(err){
              console.log("file deletion error: ",err);
            }
          })

  });

